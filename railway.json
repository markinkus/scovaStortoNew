{
  "services": [
    {
      "name": "root-install",
      "rootDir": ".",
      "buildCommand": "npm install",
      "startCommand": "echo 'done'"
    },
    {
      "name": "scovastorto-backend",
      "rootDir": "backend",
      "startCommand": "npm start",
      "buildCommand": "npm install"
    },
    {
      "name": "scovastorto-frontend",
      "rootDir": "frontend",
      "startCommand": "npm run preview",
      "buildCommand": "npm install && npm run build",
      "static": {
        "publishDir": "dist"
      }
    }
  ]
}
